<%- include('../../partials/header.ejs') %>
<div class="container">
  <hr />
  <div class="card mt-4">
    <div class="card-header">Login de admin</div>
    <div class="card-body">
      <form action="/authenticate" method="POST">
        <input
          type="email"
          class="form-control"
          name="email"
          placeholder="Email"
        />
        <br />
        <input
          type="password"
          class="form-control"
          name="password"
          placeholder="*******"
        /><br />
        <button type="submit" class="btn btn-success">Logar</button>
      </form>
    </div>
  </div>
</div>
<%- include('../../partials/footer.ejs') %>
